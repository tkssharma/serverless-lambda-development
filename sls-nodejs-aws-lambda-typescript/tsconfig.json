
{
  "compilerOptions": {
    "lib": ["es2015"],


    "preserveConstEnums": true,
    "strictNullChecks": true,
    "allowJs": true,


    "removeComments": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "lib",
    "inlineSources": true,
    "esModuleInterop": false,
    "rootDir": "./",
    "baseUrl": "./"
  },
  "include": ["./**/*.ts"],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
